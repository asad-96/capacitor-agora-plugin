{
  "name": "@wellcare/capacitor-plugin-agora-example",
  "version": "0.0.1",
  "description": "Capacitor Plugin Agora",
  "homepage": "https://github.com/wellcare/capacitor-plugin-agora#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wellcare/capacitor-plugin-agora.git"
  },
  "license": "UNLICENSED",
  "author": "Wellcare",
  "scripts": {
    "analyze": "nuxt build --analyze",
    "build:plugin": "cd ../ && yarn build && cd example && npm run clean:plugin && node build-plugin.js",
    "build": "nuxt build --modern",
    "cap:android": "yarn generate && npx cap sync android && npx cap open android",
    "cap:ios": "yarn generate && npx cap sync ios && npx cap open ios",
    "cap:open:android": "npx cap open android",
    "cap:open:ios": "npx cap open ios",
    "dev": "npm run lint && nuxt",
    "export": "nuxt export",
    "generate-capacitor": "npm run lint && nuxt generate --no-ssr && npx cap copy",
    "generate": "npm run lint && nuxt generate",
    "lint": "eslint --ext .ts,.vue . src/ --fix",
    "serve": "nuxt serve",
    "start": "nuxt start",
    "clean:plugin": "rimraf ./node_modules/@wellcare/capacitor-plugin-agora",
    "webpack": "nuxt webpack"
  },
  "dependencies": {
    "capacitor-voice-recorder": "^4.0.1",
