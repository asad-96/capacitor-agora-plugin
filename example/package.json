    "@wellcare/nuxt-module-media": "^0.3.23",
    "@wellcare/vue-authen": "1.1.11",
    "@wellcare/vue-component": "^0.1.61",
    "agora-rtc-sdk-ng": "^4.15.1",
    "axios": "^0.26.1",
    "cookie-universal-nuxt": "^2.1.5",
    "dotenv": "^16.0.1",
    "eslint-webpack-plugin": "^3.1.1",
